   "source": [
    "利用 numpy 提供的方法實作樣本標準差函數"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def std(x, y):\n",
    "    num = list(range(x, y+1))\n",
    "    avg = np.average(num)\n",
    "    sdsq = np.sum([(i - avg) ** 2 for i in num])\n",
    "    stdev = np.sqrt((sdsq / len(num)))\n",
    "    return stdev"
   ]
  },
  {
